{
  "name": "SlamNavigationApp",
  "version": "1.0.0",
  "description": "Real-time Android SLAM & Dead Reckoning App with ORB-SLAM3",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean && cd ..",
    "build-debug": "cd android && ./gradlew assembleDebug && cd ..",
    "build-release": "cd android && ./gradlew assembleRelease && cd ..",
    "install-apk": "adb install android/app/build/outputs/apk/debug/app-debug.apk"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-camera": "^4.2.1",
    "react-native-sensors": "^7.3.6",
    "react-native-fs": "^2.20.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/runtime": "^7.22.0",
    "@react-native-community/cli": "^11.0.0",
    "@react-native-community/cli-platform-android": "^11.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "keywords": [
    "react-native",
    "android",
    "slam",
    "orb-slam3",
    "dead-reckoning",
    "imu",
    "camera",
    "localization",
    "mapping",
    "navigation"
  ],
  "author": "SLAM Development Team",
  "license": "MIT",
  "private": true
}
